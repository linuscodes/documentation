*{box-sizing:border-box;margin:0;padding:0;appearance:none;-webkit-appearance:none;font-family:"acumin-pro",-apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen,Ubuntu,Cantarell,"Open Sans","Helvetica Neue",sans-serif;outline:none}.page-content{margin-left:400px;padding-top:50px;max-width:700px;padding-right:50px}@media screen and (max-width: 850px){.page-content{padding:25px;padding-top:100px;margin-left:0}}.page-content section{margin-bottom:60px}.page-content h1{margin-bottom:15px}.page-content h2,.page-content h3{margin-bottom:10px}.page-content h2.indent,.page-content h3.indent{margin-left:10px}.page-content p{color:#999;line-height:1.6;font-size:15px;margin-bottom:20px;letter-spacing:.2px}.page-content table{border-spacing:0;border-collapse:separate;width:100%}.page-content table tr td{border:1px solid #f2f2f2;border-bottom:none;padding:10px;vertical-align:center}.page-content table tr td p{margin-bottom:0}.page-content table tr td ul,.page-content table tr td ol{padding-left:20px}.page-content table tr td:first-child{border-right:none;width:33.3%}.page-content table tr td:last-child{width:66.6%}.page-content table tr:first-child td:first-child{border-radius:7px 0 0 0}.page-content table tr:first-child td:last-child{border-radius:0 7px 0 0}.page-content table tr:last-child td{border-bottom:1px solid #f2f2f2}.page-content table tr:last-child td:first-child{border-radius:0 0 0 7px}.page-content table tr:last-child td:last-child{border-radius:0 0 7px 0}pre{margin-top:20px;margin-bottom:20px;padding:15px;border-radius:7px;background:#0a0b15}code{font-family:"jetbrains-mono";font-size:14px;line-height:1.2}code.inline{padding:4px 7px;font-size:13px;border-radius:7px;background:#0a0b15;display:inline-block;transform:translateY(-1px)}code span{font-family:"jetbrains-mono";letter-spacing:.5px}code span.tag{color:#39BAE6}code span.attribute{color:#ffb454}code span.punctuation{color:#b3b1adb3}code span.string{color:#c2d94c}code span.text{color:#b3b1ad}a{color:#ff1926;text-decoration:none}a.btn{padding:11px 25px 12px 25px;display:inline-block;border-radius:7px}a.btn.primary{background:#ff1926;color:#fff}a.btn.secondary{background:#f2f2f2;color:#000}.sidebar{width:350px;height:100vh;position:fixed;top:0;left:0;background:#000;padding:50px 40px;transition:transform 0.4s cubic-bezier(0.12, 0.845, 0.305, 1);z-index:3}.sidebar h2{color:#fff}.sidebar ul{list-style:none;margin-top:40px}.sidebar ul .sidebar-link{margin-bottom:10px;display:block;width:100%}.sidebar ul .sidebar-link a{color:#fff}.sidebar ul .sidebar-link a::before{content:"";width:5px;height:5px;border-radius:50%;position:relative;display:inline-block;margin-right:5px;transform:translateY(-2px);transition:0.3s cubic-bezier(0.12, 0.845, 0.305, 1);background:#4d4d4d}.sidebar ul .sidebar-link a:hover::before{background:#ccc}.sidebar ul .sidebar-link.active a::before{background:#ff1926}@media screen and (max-width: 850px){.sidebar{transform:translateX(-100%);top:60px;padding:30px 25px}.sidebar.is-toggled{transform:translateX(0)}}.top-bar{width:100%;height:60px;padding:0 25px;position:fixed;top:0;left:0;background:#000;display:none;align-items:center;z-index:4}@media screen and (max-width: 850px){.top-bar{display:flex}}.top-bar .menu-toggle{display:none;cursor:pointer}@media screen and (max-width: 850px){.top-bar .menu-toggle{display:block}}.top-bar .menu-toggle .bars .bar{width:25px;height:1px;background:#fff;transition:transform 1s cubic-bezier(0.19, 1, 0.22, 1),opacity 0.4s ease}.top-bar .menu-toggle .bars .bar:nth-child(1){transform-origin:0 0}.top-bar .menu-toggle .bars .bar:nth-child(3){transform-origin:0 0}.top-bar .menu-toggle .bars .bar:not(:last-child){margin-bottom:5px}.breadcrumbs li{display:inline-block;margin-right:10px}

